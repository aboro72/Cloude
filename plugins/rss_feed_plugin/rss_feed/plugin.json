{
  "name": "Heise RSS Feed",
  "slug": "rss-feed",
  "version": "1.0.0",
  "author": "CloudService Team",
  "description": "Zeigt aktuelle News von heise.de als RSS-Feed Widget an.",

  "django_app": {
    "app_config": "rss_feed.apps.RssFeedConfig"
  },

  "requirements": {
    "django": ">=5.0,<6.0",
